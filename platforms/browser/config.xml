<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jakinventions.freext" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FreeXT</name>
    <description>
        FreeXT is a simple note taking app.
    </description>
    <author email="support@jakinventions.com" href="https://jakinventions.com">
        Jak Inventions
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
